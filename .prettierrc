{
  "$schema": "http://json.schemastore.org/prettierrc",
  "singleQuote": true,
  "trailingComma": "es5",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "semi": true,
  "overrides": [
    {
      "files": ["*.page.html", "*.container.html"],
      "options": {
        "parser": "angular"
      }
    }
  ]
}
